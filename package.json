  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aphrodite": "^2.3.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
